{
  "name": "mdcpy",
  "displayName": "mdcpy",
  "publisher": "cliven",
  "description": "Markdown ctrl+v paste image.",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Trisia/mdcpy.git"
  },
  "engines": {
    "vscode": "^1.54.0"
  },
  "categories": [
    "Other",
    "Keymaps"
  ],
  "keywords": [
    "markdown",
    "image",
    "paste"
  ],
  "activationEvents": [
    "onLanguage:markdown"
  ],
  "main": "./extension.js",
  "contributes": {
    "keybindings": {
      "command": "mdcpy.paste",
      "key": "ctrl+v",
      "when": "editorTextFocus && resourceLangId == markdown"
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.54.0",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  },
  "dependencies": {
    "moment": "^2.29.1"
  }
}